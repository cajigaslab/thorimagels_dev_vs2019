<UserControl x:Class="MenuLSDll.View.RemoteIPCView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:rmtipc="clr-namespace:RemoteIPCControl;assembly=RemoteIPCControl"
             Height="Auto"
             Width="Auto">
    <Grid>
        <rmtipc:RemoteIPCControlUC
            RemoteConnection ="{Binding Path=RemoteConnection}"
            RemotePCHostName ="{Binding Path=RemotePCHostName}"
            RemoteAppName ="{Binding Path=RemoteAppName}"
            LocalPCHostName ="{Binding Path=LocalPCHostName}"
            LocalPCIPv4 ="{Binding Path=LocalPCIPv4}"
            IDMode ="{Binding Path=IDMode}"
            >
        </rmtipc:RemoteIPCControlUC>
    </Grid>
</UserControl>