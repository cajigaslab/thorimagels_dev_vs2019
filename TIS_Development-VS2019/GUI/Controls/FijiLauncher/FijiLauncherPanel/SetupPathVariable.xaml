<Window x:Class="FijiLauncherPanel.SetupPathVariable"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:sp="clr-namespace:SetScriptPath;assembly=SetScriptPath"
        Title="SetupPathVariable"
        Height="670"
        Width="520"
        WindowStartupLocation="CenterOwner">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/FijiLauncherPanel;component/Button.xaml" />
                <ResourceDictionary Source="/FijiLauncherPanel;component/Window.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <StackPanel>
            <sp:UserControl1 Name="ucSetPath" />
            <Button Name="btnOK"
                    Click="btnOK_Click"
                    Content="OK"
                    HorizontalAlignment="Right"
                    Width="75"
                    Height="30"></Button>
        </StackPanel>
    </Grid>
</Window>
