                          SelectBackgroundCommand="{Binding Path=SelectBackgroundCommand, Mode=OneWay,UpdateSourceTrigger=PropertyChanged}"
                          EnableFlatField="{Binding Path=EnableFlatField, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"
                          PathFlatField="{Binding Path=PathFlatField, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"
                          SelectFlatFieldCommand="{Binding Path=SelectFlatFieldCommand, Mode=OneWay,UpdateSourceTrigger=PropertyChanged}"
                          RectangleAreaModeVisibility="{Binding Path=RectangleAreaModeVisibility, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}"
